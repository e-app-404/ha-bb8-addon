{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: Current File",
      "type": "node",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "envFile": "${workspaceFolder}/.env"
    },
    {
      "name": "Evidence: collect_stp4",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/addon/ops/evidence/collect_stp4.py",
      "cwd": "${workspaceFolder}/addon",
      "envFile": "${workspaceFolder}/.env"
    }
  ]
}
